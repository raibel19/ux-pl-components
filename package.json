{
  "name": "ux-pl-components",
  "private": false,
  "version": "0.1.0",
  "description": "Funciones para array, base64, blob, file, hash, interval, merge, mime, mobile, numbers, object, react, string, units, url",
  "keywords": [
    "typescript",
    "helpers",
    "ux-pl",
    "ux-pl-components"
  ],
  "author": "Polo",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/raibel19/ux-pl-components.git"
  },
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.config-components.d.ts",
      "default": "./dist/tailwind.config-components.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "clean": "del-cli ./dist && del-cli ./*.tgz",
    "dev": "vite --port 5173",
    "build": "npm run clean && tsc --p ./tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "tailwind-merge": "^3.3.0",
    "tslib": "^2.8.1",
    "use-debounce": "^10.0.4",
    "ux-pl": "0.1.0"
  },
  "peerDependencies": {
    "@carbon/icons-react": "^11.52.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "lucide-react": ">=0.454.0 <1.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@carbon/icons-react": "^11.60.0",
    "@eslint/js": "^9.22.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.15.23",
    "@types/react": "18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "del-cli": "^6.0.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
